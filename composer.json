{
  "name": "sadanand1991/rm-charts",
  "description": "RM Charts widget",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "phpcompatibility/php-compatibility": "^9.3",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "scripts": {
    "cs-check": "phpcs --standard=WordPress --ignore=vendor,node_modules --extensions=php .",
    "cs-fix": "phpcbf --standard=WordPress --ignore=vendor,node_modules --extensions=php ."
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
